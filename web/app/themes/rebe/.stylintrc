{
	"blocks": false,
	"brackets": "never",
	"colons": "never",
	"colors": "always",
	"commaSpace": "always",
	"commentSpace": "always",
	"cssLiteral": "never",
	"depthLimit": false,
	"duplicates": true,
	"efficient": "always",
	"extendPref": false,
	"globalDupe": false,
	"indentPref": false,
	"leadingZero": "never",
	"maxErrors": false,
	"maxWarnings": false,
	"mixed": false,
	"namingConvention": "BEM",
	"namingConventionStrict": false,
	"none": "never",
	"noImportant": true,
	"parenSpace": "never",
	"placeholders": "false",
	"prefixVarsWithDollar": "always",
	"quotePref": "single",
	"semicolons": "never",
	"sortOrder": [
		"display",
		"visibility",
		"position",
		"z-index",
		"float",
		"margin",
		"padding",
		"min-width",
		"min-height",
		"max-width",
		"max-height",
		"size",
		"width",
		"height",
		"font-family",
		"font-weight",
		"font-style",
		"font-size",
		"text-align",
		"text-transform",
		"text-decoration",
		"line-height",
		"color",
		"background",
		"background-color",
		"border"
	],
	"stackedProperties": "never",
	"trailingWhitespace": "never",
	"universal": false,
	"valid": true,
	"zeroUnits": "never",
	"zIndexNormalize": false
}
