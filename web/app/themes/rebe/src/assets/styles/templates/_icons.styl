/**
 * Iconfont icons components
 *
 *    Generated by `gulp-iconfont`. See `themes/rebe/gulpfile.js`
 *    @link: https://www.npmjs.com/package/gulp-iconfont
 */
<% _.each(glyphs, function(glyph) { %>
.<%= className %>-<%= glyph.name %>:before
  content: "\<%= glyph.unicode[0].charCodeAt(0).toString(16).toUpperCase() %>" !important
<% }); %>
