/**
 * Iconfont icon object
 *
 *    Generated by `gulp-iconfont`. See `themes/rebe/gulpfile.js`
 *    @link: https://www.npmjs.com/package/gulp-iconfont
 */
@font-face
  font-family "ReBeIconFont"
  src url('../fonts/ReBeIconFont.eot')
  src url('../fonts/ReBeIconFont.eot?#iefix') format('eot'),
    url('../fonts/ReBeIconFont.woff') format('woff'),
    url('../fonts/ReBeIconFont.ttf') format('truetype'),
    url('../fonts/ReBeIconFont.svg#ReBeIconFont') format('svg')
  font-weight normal
  font-style normal

.o-icon:before
  display block
  speak none
  font-family "ReBeIconFont"
  font-style normal
  font-weight normal
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale